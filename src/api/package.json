	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc --pretty",
		"dev": "NODE_ENV=dev nodemon -w dist -w ../lib/dist --delay 20ms --inspect=2301 dist/index.js",
